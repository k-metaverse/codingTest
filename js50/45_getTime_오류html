<!-- js -->
<script>
const theDate=new Date()
console.log(theDate.getTime())
console.log(60*60)//1시간->초
console.log(60*60*24)//하루->초
console.log(60*60*24*365)//1년->초
console.log(60*60*24*365*1000)//1년->초 ms
console.log(Math.floor(theDate.getTime()/(60*60*24*365*1000))+1973)
</script>
	
